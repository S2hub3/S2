local StarterGui = game:GetService("StarterGui")

-- Function to create a notification
local function sendNotification(title, text, duration)
   StarterGui:SetCore("SendNotification", {
       Title = title;
       Text = text;
       Duration = duration or 5;
   })
end

-- Example usage of the notification function
sendNotification("Ø§Ù„Ø³ÙƒØ±Ø¨Øª Ù„Ø³Ø§ ØªØ­Øª Ø§Ù„ØªØ·ÙˆÙŠØ±", "Ø§Ù‚ØªÙ„ Ù†ÙØ³Ùƒ Ø§Ø°Ø§ ØµØ§Ø±Øª Ù…Ø¹Ùƒ Ù…Ø´Ù„Ù‡ ÙÙŠ Ø§Ù„Ù†Ø·", 9)
sendNotification("ØµÙ„Ù‰ Ø¹Ù„Ù‰ Ù…Ø­Ù…Ø¯", "Ø¹Ù„ÙŠÙ‡ Ø§ÙØ¶Ù„ Ø§Ù„ØµÙ„Ø§Ø© ÙˆØ§Ù„Ø³Ù„Ø§Ù…", 9)

local Players = game:GetService("Players")
local StarterGui = game:GetService("StarterGui")

-- Add reset function
local function resetAvatar()
   local player = Players.LocalPlayer
   if player.Character then
       local humanoid = player.Character:FindFirstChild("Humanoid")
       if humanoid then
           humanoid.Health = 0
           StarterGui:SetCore("SendNotification", {
               Title = "Avatar Reset",
               Text = "Character Reset Successfully",
               Duration = 3
           })
       end
   end
end

-- Execute reset
resetAvatar()

